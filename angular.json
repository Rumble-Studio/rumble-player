{
	"version": 1,
	"cli": {
		"defaultCollection": "@nrwl/angular",
		"analytics": false
	},
	"defaultProject": "demo-angular",
	"schematics": {
		"@nrwl/angular:application": {
			"style": "scss",
			"linter": "eslint",
			"unitTestRunner": "jest",
			"e2eTestRunner": "cypress"
		},
		"@nrwl/angular:library": {
			"style": "scss",
			"linter": "eslint",
			"unitTestRunner": "jest"
		},
		"@nrwl/angular:component": {
			"style": "scss"
		},
		"@nrwl/react": {
			"application": {
				"style": "scss",
				"linter": "eslint",
				"babel": true
			},
			"component": {
				"style": "scss"
			},
			"library": {
				"style": "scss",
				"linter": "eslint"
			}
		}
	},
	"projects": {
		"demo-angular": {
			"projectType": "application",
			"root": "apps/demo-angular",
			"sourceRoot": "apps/demo-angular/src",
			"prefix": "rumble-player",
			"architect": {
				"build": {
					"builder": "@angular-devkit/build-angular:browser",
					"options": {
						"outputPath": "dist/apps/demo-angular",
						"index": "apps/demo-angular/src/index.html",
						"main": "apps/demo-angular/src/main.ts",
						"polyfills": "apps/demo-angular/src/polyfills.ts",
						"tsConfig": "apps/demo-angular/tsconfig.app.json",
						"aot": true,
						"assets": [
							"apps/demo-angular/src/favicon.ico",
							"apps/demo-angular/src/assets"
						],
						"styles": [
							"./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
							"apps/demo-angular/src/styles.scss"
						],
						"scripts": []
					},
					"configurations": {
						"production": {
							"fileReplacements": [
								{
									"replace": "apps/demo-angular/src/environments/environment.ts",
									"with": "apps/demo-angular/src/environments/environment.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"buildOptimizer": true,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "5mb"
								},
								{
									"type": "anyComponentStyle",
									"maximumWarning": "6kb",
									"maximumError": "10kb"
								}
							]
						}
					}
				},
				"serve": {
					"builder": "@angular-devkit/build-angular:dev-server",
					"options": {
						"browserTarget": "demo-angular:build"
					},
					"configurations": {
						"production": {
							"browserTarget": "demo-angular:build:production"
						}
					}
				},
				"extract-i18n": {
					"builder": "@angular-devkit/build-angular:extract-i18n",
					"options": {
						"browserTarget": "demo-angular:build"
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": [
							"apps/demo-angular/src/**/*.ts",
							"apps/demo-angular/src/**/*.html"
						]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["coverage/apps/demo-angular"],
					"options": {
						"jestConfig": "apps/demo-angular/jest.config.js",
						"passWithNoTests": true
					}
				}
			}
		},
		"demo-angular-e2e": {
			"root": "apps/demo-angular-e2e",
			"sourceRoot": "apps/demo-angular-e2e/src",
			"projectType": "application",
			"architect": {
				"e2e": {
					"builder": "@nrwl/cypress:cypress",
					"options": {
						"cypressConfig": "apps/demo-angular-e2e/cypress.json",
						"tsConfig": "apps/demo-angular-e2e/tsconfig.e2e.json",
						"devServerTarget": "demo-angular:serve"
					},
					"configurations": {
						"production": {
							"devServerTarget": "demo-angular:serve:production"
						}
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["apps/demo-angular-e2e/**/*.{js,ts}"]
					}
				}
			}
		},
		"demo-react": {
			"root": "apps/demo-react",
			"sourceRoot": "apps/demo-react/src",
			"projectType": "application",
			"architect": {
				"build": {
					"builder": "@nrwl/web:build",
					"outputs": ["{options.outputPath}"],
					"options": {
						"outputPath": "dist/apps/demo-react",
						"index": "apps/demo-react/src/index.html",
						"main": "apps/demo-react/src/main.tsx",
						"polyfills": "apps/demo-react/src/polyfills.ts",
						"tsConfig": "apps/demo-react/tsconfig.app.json",
						"assets": [
							"apps/demo-react/src/favicon.ico",
							"apps/demo-react/src/assets"
						],
						"styles": ["apps/demo-react/src/styles.scss"],
						"scripts": [],
						"webpackConfig": "@nrwl/react/plugins/webpack"
					},
					"configurations": {
						"production": {
							"fileReplacements": [
								{
									"replace": "apps/demo-react/src/environments/environment.ts",
									"with": "apps/demo-react/src/environments/environment.prod.ts"
								}
							],
							"optimization": true,
							"outputHashing": "all",
							"sourceMap": false,
							"extractCss": true,
							"namedChunks": false,
							"extractLicenses": true,
							"vendorChunk": false,
							"budgets": [
								{
									"type": "initial",
									"maximumWarning": "2mb",
									"maximumError": "5mb"
								}
							]
						}
					}
				},
				"serve": {
					"builder": "@nrwl/web:dev-server",
					"options": {
						"buildTarget": "demo-react:build"
					},
					"configurations": {
						"production": {
							"buildTarget": "demo-react:build:production"
						}
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["apps/demo-react/**/*.{ts,tsx,js,jsx}"]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["coverage/apps/demo-react"],
					"options": {
						"jestConfig": "apps/demo-react/jest.config.js",
						"passWithNoTests": true
					}
				}
			}
		},
		"demo-react-e2e": {
			"root": "apps/demo-react-e2e",
			"sourceRoot": "apps/demo-react-e2e/src",
			"projectType": "application",
			"architect": {
				"e2e": {
					"builder": "@nrwl/cypress:cypress",
					"options": {
						"cypressConfig": "apps/demo-react-e2e/cypress.json",
						"tsConfig": "apps/demo-react-e2e/tsconfig.e2e.json",
						"devServerTarget": "demo-react:serve"
					},
					"configurations": {
						"production": {
							"devServerTarget": "demo-react:serve:production"
						}
					}
				},
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["apps/demo-react-e2e/**/*.{js,ts}"]
					}
				}
			}
		},
		"rumble-player": {
			"root": "libs/rumble-player",
			"sourceRoot": "libs/rumble-player/src",
			"projectType": "library",
			"architect": {
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["libs/rumble-player/**/*.ts"]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["coverage/libs/rumble-player"],
					"options": {
						"jestConfig": "libs/rumble-player/jest.config.js",
						"passWithNoTests": true
					}
				},
				"build": {
					"builder": "@nrwl/node:package",
					"outputs": ["{options.outputPath}"],
					"options": {
						"outputPath": "dist/libs/rumble-player",
						"tsConfig": "libs/rumble-player/tsconfig.lib.json",
						"packageJson": "libs/rumble-player/package.json",
						"main": "libs/rumble-player/src/index.ts",
						"assets": ["libs/rumble-player/*.md"]
					}
				}
			}
		},
		"dom": {
			"root": "libs/dom",
			"sourceRoot": "libs/dom/src",
			"projectType": "library",
			"architect": {
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["libs/dom/**/*.ts"]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["coverage/libs/dom"],
					"options": {
						"jestConfig": "libs/dom/jest.config.js",
						"passWithNoTests": true
					}
				},
				"build": {
					"builder": "@nrwl/node:package",
					"outputs": ["{options.outputPath}"],
					"options": {
						"outputPath": "dist/libs/dom",
						"tsConfig": "libs/dom/tsconfig.lib.json",
						"packageJson": "libs/dom/package.json",
						"main": "libs/dom/src/index.ts",
						"assets": ["libs/dom/*.md"]
					}
				}
			}
		},
		"service": {
			"root": "libs/service",
			"sourceRoot": "libs/service/src",
			"projectType": "library",
			"architect": {
				"lint": {
					"builder": "@nrwl/linter:eslint",
					"options": {
						"lintFilePatterns": ["libs/service/**/*.ts"]
					}
				},
				"test": {
					"builder": "@nrwl/jest:jest",
					"outputs": ["coverage/libs/service"],
					"options": {
						"jestConfig": "libs/service/jest.config.js",
						"passWithNoTests": true
					}
				},
				"build": {
					"builder": "@nrwl/node:package",
					"outputs": ["{options.outputPath}"],
					"options": {
						"outputPath": "dist/libs/service",
						"tsConfig": "libs/service/tsconfig.lib.json",
						"packageJson": "libs/service/package.json",
						"main": "libs/service/src/index.ts",
						"assets": ["libs/service/*.md"]
					}
				}
			}
		}
	}
}
