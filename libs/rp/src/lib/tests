describe('rumble player', () => {
  it('calling pause method while playing should change playing status to false', () => {
    const rp = new RumblePlayer()
    // rp.connectedCallback()
    rp.play()
    rp.pause()
    expect(rp.isPlaying).toEqual(false);
  });
