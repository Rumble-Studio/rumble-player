		"@nrwl/node": "12.0.8",
		"@nrwl/nx-cloud": "latest",
		"@nrwl/react": "11.4.0",
		"@nrwl/tao": "11.4.0",
		"@nrwl/web": "11.4.0",
		"@nrwl/workspace": "11.4.0",
		"@testing-library/react": "11.1.2",
		"@types/howler": "^2.2.1",
